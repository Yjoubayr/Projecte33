      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TaulerDeControlRM"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageCrea">

    <Grid>
        <StackPanel>
            <TextBlock Text="Crea" Margin="5"/>
            <ComboBox x:Name="elementComboBox" Margin="5" SelectionChanged="ComboBox_SelectionChanged">
            </ComboBox>
            <Frame x:Name="frameCrear" NavigationUIVisibility="Hidden"/>
        </StackPanel>
    </Grid>
</Page>

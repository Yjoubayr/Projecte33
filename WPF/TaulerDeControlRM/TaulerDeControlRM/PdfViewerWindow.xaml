<!-- PdfViewerWindow.xaml -->
<Window x:Class="TaulerDeControlRM.PdfViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PDF Viewer" Height="450" Width="800">
    <Grid>
        <Frame x:Name="pdfFrame" NavigationUIVisibility="Hidden"/>
        <Button Content="Close" Click="CloseButton_Click" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,10,10,0"/>
    </Grid>
</Window>


